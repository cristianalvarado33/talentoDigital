Proceso promedio_1
	
	definir num1, num2, num3, total, promedio como enteros;
	
	
	Escribir "ingrese la primera nota";
	leer num1;
	Escribir "ingrese la segunda nota";
	leer num2;
	Escribir "ingrese la tercera nota";
	leer num3;
	
	total<-num1+num2+num3;
	promedio<-total/3;
	
	escribir "su promedio es: ", promedio;
	
FinProceso
